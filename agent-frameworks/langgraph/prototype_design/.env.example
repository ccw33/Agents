# 高保真原型设计Agent环境配置文件
# 请复制此文件为 .env 并填入您的实际配置

# DashScope API配置 (阿里云通义千问)
DASHSCOPE_API_KEY=your_dashscope_api_key_here
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# 模型配置
DESIGNER_MODEL=qwen-coder-plus-latest
VALIDATOR_MODEL=qwen-vl-plus

# Token限制配置
MAX_INPUT_TOKENS=8000
VALIDATOR_MAX_INPUT_TOKENS=128000
MAX_OUTPUT_TOKENS=4000

# LangSmith配置 (可选，用于监控和追踪)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=prototype-design-agent

# 服务器配置
DEFAULT_SERVER_PORT=8000

# 其他配置
ITERATION_LIMIT=5
ENABLE_DEBUG=false
